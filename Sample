kubectl -n airflow exec -it airflow-scheduler-859b5c99b6-dfxl2 -- curl -v http://127.0.0.1:8793/health || echo "curl failed"

kubectl -n airflow get pods --selector=app=airflow-web --no-headers -o custom-columns=NAME:.metadata.name
# or fallback:
kubectl -n airflow get pods | findstr /I web


kubectl -n airflow exec -it <web-pod> -- curl -v http://airflow-scheduler:8793/health || echo "web->scheduler curl failed"
