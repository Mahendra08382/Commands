# replace <LOG_PATH> with the full path you copied (include leading /log/...). Wrap in single quotes.
$LOG_PATH = '/log/dag_id=sample_k8s_dag/run_id=manual__2025-09-09T02:33:54.100502+00:00/task_id=hello/attempt=1.log'
kubectl -n airflow exec $WS -- sh -c "curl -v --max-time 10 'http://airflow-scheduler:8793$LOG_PATH' 2>&1 | sed -n '1,200p'"
