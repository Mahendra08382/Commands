kubectl -n airflow get pods --show-labels
kubectl -n airflow get pods --show-labels | findstr /I scheduler
\

kubectl -n airflow patch svc airflow-scheduler -p "{\"spec\":{\"selector\":{\"app.kubernetes.io/component\":\"scheduler\"}}}"


kubectl -n airflow patch svc airflow-scheduler -p "{\"spec\":{\"selector\":{\"airflow-component\":\"scheduler\"}}}"
