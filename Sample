kubectl get ingress -n airflow -o yaml


$web = kubectl get pods -n airflow --no-headers | Select-String "web" | ForEach-Object { ($_ -split '\s+')[0] } | Select-Object -First 1
kubectl exec -n airflow $web -- bash -c "curl -v -H 'Authorization: Bearer test' http://airflow-scheduler:8793/ || echo 'curl-failed'"
