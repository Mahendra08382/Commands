kubectl cp "C:\Windows\Temp\airflow-logs.tgz" "$web:/tmp/airflow-logs.tgz" -n airflow

kubectl exec -n airflow $web -- bash -c "mkdir -p /opt/airflow/logs && tar xzf /tmp/airflow-logs.tgz -C /opt/airflow && rm -f /tmp/airflow-logs.tgz"



